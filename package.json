{
  "name": "goodseed-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "# Development": "════════════════════════════════════════════════════",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "postinstall": "prisma generate",
    "# Docker & Redis": "════════════════════════════════════════════════════",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi local",
    "redis:up": "docker-compose up -d redis",
    "redis:down": "docker-compose stop redis",
    "redis:restart": "docker-compose restart redis",
    "redis:cli": "docker exec -it goodseed-redis redis-cli",
    "redis:flush": "docker exec goodseed-redis redis-cli FLUSHALL",
    "# Worker": "════════════════════════════════════════════════════",
    "worker": "tsx lib/workers/worker.ts",
    "worker:up": "docker-compose up -d worker",
    "worker:down": "docker-compose stop worker",
    "worker:restart": "docker-compose restart worker",
    "worker:logs": "docker-compose logs -f worker",
    "# Database (Prisma)": "════════════════════════════════════════════════════",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed-dispensaries.ts",
    "db:seed:dispensaries": "tsx scripts/seed-dispensaries.ts",
    "db:reset": "prisma migrate reset",
    "db:backup": "bash scripts/database/backup-database.sh",
    "# Production": "════════════════════════════════════════════════════",
    "cron:start": "tsx scripts/cron/cron-server.ts",
    "# Quick Start": "════════════════════════════════════════════════════",
    "setup": "pnpm install && pnpm db:generate && pnpm docker:up && pnpm db:push",
    "start:all": "pnpm redis:up && pnpm worker:up && pnpm dev",
    "stop:all": "pnpm docker:down",
    "# Git Helpers": "════════════════════════════════════════════════════",
    "push:origin": "git push origin $(git branch --show-current)",
    "push:customer": "git push customer $(git branch --show-current)",
    "push:all": "git push origin $(git branch --show-current) && git push customer $(git branch --show-current)"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-brands-svg-icons": "6.5.2",
    "@fortawesome/free-regular-svg-icons": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-email/components": "^1.0.4",
    "@react-email/render": "^2.0.2",
    "@tanstack/query-async-storage-persister": "^5.90.22",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-query-persist-client": "^5.90.22",
    "@upstash/redis": "^1.36.1",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bull": "^4.16.5",
    "cheerio": "1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crawlee": "3.16.0",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.555.0",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.71.1",
    "react-spinners": "^0.17.0",
    "resend": "^6.7.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.9.19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "prettier": "^3.7.4",
    "prisma": "^7.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
