{
  "family": "goodseed-free-app",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::396260462264:role/goodseed-free-ecs-task-execution-role",
  "taskRoleArn": "arn:aws:iam::396260462264:role/goodseed-free-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "goodseed-app",
      "image": "396260462264.dkr.ecr.us-east-1.amazonaws.com/goodseed-free:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgresql://goodseed_user:GoodSeed2024SecureDB@goodseed-free-db.cq7eog2a85qn.us-east-1.rds.amazonaws.com:5432/goodseed_free?sslmode=disable"
        },
        {
          "name": "AUTH_SECRET",
          "value": "goodseed-nextauth-secret-key-production-2024"
        },
        {
          "name": "AUTH_URL",
          "value": "https://lembooking.com"
        },
        {
          "name": "AUTH_TRUST_HOST",
          "value": "true"
        },
        {
          "name": "NEXT_PUBLIC_APP_URL", 
          "value": "https://lembooking.com"
        },
        {
          "name": "REDIS_HOST",
          "value": "goodseed-free-redis.lxu2zc.ng.0001.use1.cache.amazonaws.com"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "RESEND_API_KEY",
          "value": "REPLACE_WITH_RESEND_API_KEY"
        },
        {
          "name": "RESEND_FROM_EMAIL",
          "value": "support@example.com"
        },
        {
          "name": "AUTH_GOOGLE_ID",
          "value": "REPLACE_WITH_GOOGLE_ID"
        },
        {
          "name": "AUTH_GOOGLE_SECRET",
          "value": "REPLACE_WITH_GOOGLE_SECRET"
        },
        {
          "name": "AUTH_FACEBOOK_ID",
          "value": "REPLACE_WITH_FACEBOOK_ID"
        },
        {
          "name": "AUTH_FACEBOOK_SECRET",
          "value": "REPLACE_WITH_FACEBOOK_SECRET"
        },
        {
          "name": "NEXT_PUBLIC_DEMO_PASSWORD",
          "value": "demo-password"
        },
        {
          "name": "CRON_SECRET",
          "value": "REPLACE_WITH_CRON_SECRET"
        },
        {
          "name": "CLOUDFLARE_ZONE_ID",
          "value": "REPLACE_WITH_CF_ZONE_ID"
        },
        {
          "name": "CLOUDFLARE_API_TOKEN",
          "value": "REPLACE_WITH_CF_API_TOKEN"
        },
        {
          "name": "CLOUDFLARE_DOMAIN",
          "value": "lembooking.com"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/goodseed-free/app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "wget --no-verbose --tries=1 --spider http://localhost:3000/api/health || exit 1"
        ],
        "interval": 30,
        "timeout": 10,
        "retries": 3,
        "startPeriod": 120
      }
    }
  ]
}